# =============================================================================
# ARIA & Voice Chat Gemini - Environment Variables
# Copy this file to .env and fill in your values
# =============================================================================

# Google Gemini API Keys (get from https://aistudio.google.com/apikey)
# You can use multiple keys for rotation (google_api_1, google_api_2, etc.)
google_api=your_gemini_api_key_here
# google_api_1=your_first_api_key
# google_api_2=your_second_api_key

# ESP32-CAM IP Address (find using network scanner or serial monitor)
ESP32_CAM_IP=192.168.1.100

# Yeelight Smart Bulb IP Address
YEELIGHT_IP=192.168.1.101

# OpenWeather API Key (get from https://openweathermap.org/api)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Default city for weather
DEFAULT_CITY=Almaty

# Qdrant Vector Database (for RAG memory)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=soul
